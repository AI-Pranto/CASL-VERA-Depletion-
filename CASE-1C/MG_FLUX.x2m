INTEGER nstep := 39 ;
INTEGER nauto := 39 ;
INTEGER iauto :=  1 ;
INTEGER valstep valauto ;
REAL    evobeg evoend step2 stepauto ;

StepList := UTL: :: CREA 'ListBU' <<nstep>> =
    10.00     250.00   500.00   1000.00   2000.00
  3000.00   4000.00   5000.00   6000.00   7000.00
