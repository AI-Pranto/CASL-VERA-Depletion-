 52500.00  55000.00  57500.00  60000.00
;

StepList := UTL: StepList :: CREA 'ListAutop' <<nauto>> =
    10.00     250.00   500.00   1000.00   2000.00
  3000.00   4000.00   5000.00   6000.00   7000.00
